{
  "name": "pokedex",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prepare": "cd .. && husky frontend/.husky",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:staged": "lint-staged",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "ts:check": "./node_modules/.bin/tsc -p . --noEmit",
    "ts:watch": "./node_modules/.bin/tsc -p . --noEmit --watch",
    "prettier": "./node_modules/.bin/prettier --write 'src/**/*.{ts,tsx}'",
    "prettier:staged": "pretty-quick --staged",
    "check:style": "npm run prettier:staged && npm run lint",
    "check:health": "npm run test && npm run build",
    "check:all": "npm run check:style && npm run check:health",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build"
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.0",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/nextjs": "^8.1.11",
    "@storybook/react": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "pretty-quick": "^4.0.0",
    "storybook": "^8.1.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
